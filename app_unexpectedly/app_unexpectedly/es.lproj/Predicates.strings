/* Predicate Editor */

"%[All]@ of the following are true" = "%1$[All]@ of the following are true";
"%[Any]@ of the following are true" = "%1$[Any]@ of the following are true";

"%[processName]@ %[is]@ %@" = "%1$[Process Name]@ %2$[es]@ %3$@";
"%[processName]@ %[is not]@ %@" = "%1$[Process Name]@ %2$[no es]@ %3$@";
"%[processName]@ %[begins with]@ %@" = "%1$[Process Name]@ %2$[empieza por]@ %3$@";
"%[processName]@ %[ends with]@ %@" = "%1$[Process Name]@ %2$[acaba en]@ %3$@";
"%[processName]@ %[contains]@ %@" = "%1$[Process Name]@ %2$[contiene]@ %3$@";

"%[header.bundleIdentifier]@ %[is]@ %@" = "%1$[Identifier]@ %2$[es]@ %3$@";
"%[header.bundleIdentifier]@ %[is not]@ %@" = "%1$[Identifier]@ %2$[no es]@ %3$@";
"%[header.bundleIdentifier]@ %[begins with]@ %@" = "%1$[Identifier]@ %2$[empieza por]@ %3$@";
"%[header.bundleIdentifier]@ %[ends with]@ %@" = "%1$[Identifier]@ %2$[ends with]@ %3$@";
"%[header.bundleIdentifier]@ %[contains]@ %@" = "%1$[Identifier]@ %2$[contiene]@ %3$@";

"%[header.executablePath]@ %[is]@ %@" = "%1$[Executable Path]@ %2$[es]@ %3$@";
"%[header.executablePath]@ %[is not]@ %@" = "%1$[Executable Path]@ %2$[no es]@ %3$@";
"%[header.executablePath]@ %[begins with]@ %@" = "%1$[Executable Path]@ %2$[empieza por]@ %3$@";
"%[header.executablePath]@ %[ends with]@ %@" = "%1$[Executable Path]@ %2$[acaba en]@ %3$@";
"%[header.executablePath]@ %[contains]@ %@" = "%1$[Executable Path]@ %2$[contiene]@ %3$@";

"%[exceptionInformation.crashedThreadName]@ %[is]@ %@" = "%1$[Crashed Thread Name]@ %2$[es]@ %3$@";
"%[exceptionInformation.crashedThreadName]@ %[is not]@ %@" = "%1$[Crashed Thread Name]@ %2$[no es]@ %3$@";
"%[exceptionInformation.crashedThreadName]@ %[begins with]@ %@" = "%1$[Crashed Thread Name]@ %2$[empieza por]@ %3$@";
"%[exceptionInformation.crashedThreadName]@ %[ends with]@ %@" = "%1$[Crashed Thread Name]@ %2$[acaba en]@ %3$@";
"%[exceptionInformation.crashedThreadName]@ %[contains]@ %@" = "%1$[Crashed Thread Name]@ %2$[contiene]@ %3$@";

"%[dateTime]@ %[is less than or equal to]@ %@" = "%1$[Fecha]@ %2$[es anterior a]@ %3$@";
"%[dateTime]@ %[is greater than or equal to]@ %@" = "%1$[Fecha]@ %2$[es posterior a]@ %3$@";

